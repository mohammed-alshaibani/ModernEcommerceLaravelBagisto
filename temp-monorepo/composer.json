{
    "name": "bagisto/monorepo",
    "description": "Bagisto Monorepo",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Bagisto Team",
            "email": "support@bagisto.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "ext-calendar": "*",
        "ext-curl": "*",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "ext-openssl": "*",
        "ext-pdo": "*",
        "ext-pdo_mysql": "*",
        "ext-tokenizer": "*",
        "astrotomic/laravel-translatable": "^11.0.0",
        "bagisto/image-cache": "dev-master",
        "barryvdh/laravel-dompdf": "^2.0.0",
        "diglactic/laravel-breadcrumbs": "^9.0",
        "elasticsearch/elasticsearch": "^8.10",
        "enshrined/svg-sanitize": "^0.22.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^2.4",
        "kalnoy/nestedset": "^6.0",
        "khaled.alshamaa/ar-php": "^6.0.0",
        "konekt/concord": "^1.2",
        "laravel/framework": "^11.0",
        "laravel/octane": "^2.3",
        "laravel/sanctum": "^4.0",
        "laravel/socialite": "^5.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^4.0",
        "maatwebsite/excel": "^3.1.46",
        "mpdf/mpdf": "^8.2",
        "nesbot/carbon": "^2.72.2",
        "openai-php/laravel": "^0.10.1",
        "paypal/paypal-checkout-sdk": "1.0.1",
        "predis/predis": "^2.2",
        "prettus/l5-repository": "^2.6",
        "pusher/pusher-php-server": "^7.0",
        "shetabit/visitor": "^4.1",
        "spatie/laravel-responsecache": "^7.4",
        "spatie/laravel-sitemap": "^7.3",
        "stevebauman/purify": "^6.3"
    },
    "require-dev": {
        "bagisto/laravel-datafaker": "2.3.*",
        "barryvdh/laravel-debugbar": "^3.8",
        "fakerphp/faker": "^1.23",
        "laravel/pint": "^1.19",
        "mockery/mockery": "^1.6",
        "nunomaduro/collision": "^8.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "phpunit/phpunit": "^11.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "projects/my-bagisto-store/app/",
            "Database\\Factories\\": "projects/my-bagisto-store/database/factories/",
            "Database\\Seeders\\": "projects/my-bagisto-store/database/seeders/",
            "Webkul\\Admin\\": "packages/admin/Admin/src",
            "Webkul\\Attribute\\": "packages/modules/Attribute/src",
            "Webkul\\Booking\\": "packages/modules/Booking/src",
            "Webkul\\BookingProduct\\": "packages/modules/Booking/src",
            "Webkul\\CartRule\\": "packages/modules/CartRule/src",
            "Webkul\\CatalogRule\\": "packages/modules/CatalogRule/src",
            "Webkul\\Category\\": "packages/modules/Category/src",
            "Webkul\\Checkout\\": "packages/modules/Checkout/src",
            "Webkul\\CMS\\": "packages/modules/CMS/src",
            "Webkul\\Core\\": "packages/core/Core/src",
            "Webkul\\Customer\\": "packages/modules/Customer/src",
            "Webkul\\DataGrid\\": "packages/core/DataGrid/src",
            "Webkul\\DataTransfer\\": "packages/core/DataTransfer/src",
            "Webkul\\DebugBar\\": "packages/modules/DebugBar/src",
            "Webkul\\FPC\\": "packages/modules/FPC/src",
            "Webkul\\GDPR\\": "packages/modules/GDPR/src",
            "Webkul\\Installer\\": "packages/modules/Installer/src",
            "Webkul\\Inventory\\": "packages/modules/Inventory/src",
            "Webkul\\MagicAI\\": "packages/modules/MagicAI/src",
            "Webkul\\Marketing\\": "packages/modules/Marketing/src",
            "Webkul\\Notification\\": "packages/modules/Notification/src",
            "Webkul\\Paypal\\": "packages/payment/Paypal/src",
            "Webkul\\Payment\\": "packages/payment/Payment/src",
            "Webkul\\Product\\": "packages/modules/Product/src",
            "Webkul\\Rule\\": "packages/core/Rule/src",
            "Webkul\\Sales\\": "packages/modules/Sales/src",
            "Webkul\\Shipping\\": "packages/shipping/Shipping/src",
            "Webkul\\Shop\\": "packages/frontend/Shop/src",
            "Webkul\\Sitemap\\": "packages/modules/Sitemap/src",
            "Webkul\\SocialLogin\\": "packages/auth/SocialLogin/src",
            "Webkul\\SocialShare\\": "packages/modules/SocialShare/src",
            "Webkul\\Tax\\": "packages/modules/Tax/src",
            "Webkul\\Theme\\": "packages/theme/Theme/src",
            "Webkul\\User\\": "packages/core/User/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "projects/my-bagisto-store/tests/"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "./packages/*"
        },
        {
            "type": "composer",
            "url": "https://packagist.org"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    }
}
